{
  "manifest_version": 3,
  "name": "Active Recall",
  "description": "in dev",
  "version": "1.0.0",
  "icons": {
  },
  "action" : {
    "default_popup": "popup.html",
    "default_title": "React Boilerplate"
  },
  "permissions": [
    "storage", "activeTab"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://*.google.com/*", "https://*/*", "http://*/*"],
      "js": ["content.js"]
    }
  ]
}